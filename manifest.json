{
    "manifest_version": 2,
    "name": "一根青菜",
    "version": "1.0.1",
    "description": "__MSG_pluginDesc__",
    "content_security_policy": "script-src 'self' blob: filesystem: 'unsafe-inline' 'unsafe-eval' 'sha256-EBNqxohkvOoz3j+BhK2CjKd5GV6ER5ODsh4WPotfz5o=' 'sha256-SI3nw33GJj4a4p0h0pGFlktYng+0TwGxuNv/MmY1U9c=' https://*.cloudflare.com; object-src 'self'",
	"default_locale": "zh_CN",
	"author": "FredZ",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "nativeMessaging",
		"cookies",
        "storage",
        "<all_urls>",
        "*://*.taobao.com/*",
        "*://*.alibaba.com/*",
        "*://*.aliyun.com/*"
    ],
    "browser_action": {
        "default_icon": "img/veg.png",
        "default_title": "for you specially",
        "default_popup": "/main/popup.html",
        "badge": "Hello dear"
    },
    "homepage_url": "https://i.alibaba.com",
    "options_ui": {
        "page": "/main/option.html"
    },
    "icons": {
        "48": "img/veg.png"
    },
    "web_accessible_resources": [
    ],
    "background": {
        "scripts": [
            "/base/network.js",
            "/base/md5.js",
            "/fetch/rpc.js",
            "/fetch/ma.js",
            "/fetch/aliyun.js",
            "/main/extensionPromisify.js",
            "/main/tasks/taskMa.js",
            "/main/bgHandler.js",
            "/main/bg.js"
        ]
    },
    "devtools_page": "",
    "content_scripts": [
        {
            "matches": [
                "https://activity.alibaba.com/pc/developer.html*"
            ],
            "all_frames": false,
            "js": [
                "/main/msgHandler.js",
                "/pages/ma/maDev.js"
            ],
            "run_at": "document_idle"
        },
        {
            "matches": [
                "https://message.alibaba.com/message/maDetail.htm*"
            ],
            "all_frames": false,
            "js": [
                "/base/proto_string.js",
                "/base/proto_date.js",
                "/base/qrcode.min.js",
                "/main/msgHandler.js",
                "/pages/ma/maInquiry.js"
            ],
            "run_at": "document_idle"
        },
        {
            "matches": [
                "https://i.alibaba.com/*"
            ],
            "all_frames": false,
            "js": [
                "/pages/ma/maHome.js"
            ],
            "run_at": "document_idle"
        },
        {
            "matches": [
                "http://localhost:3000/*"
            ],
            "all_frames": false,
            "js": [
                "/public/index.js",
                "/public/fERP.js"
            ],
            "run_at": "document_start"
        }
    ]
}